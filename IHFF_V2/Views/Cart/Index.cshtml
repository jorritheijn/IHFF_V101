@model IHFF_V2.Models.CartItem

@using IHFF_V2.Models

@{
    ViewBag.Title = "Index";
}

<h2>Shopping Cart</h2>

@foreach (CartItem ci in (List<CartItem>)Session["cart"])
{
    <tr>
        <td>@ci.Name</td>
        <td>@ci.Time</td>
        <td>@ci.Location</td>
        <td>@ci.Price</td>
    </tr>
}
